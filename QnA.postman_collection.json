		{
			"name": "Create Skillset",
			"request": {
				"method": "PUT",
				"header": [
					{
						"key": "api-key",
						"value": "{{search-service-admin-key}}",
						"type": "text"
					},
					{
						"key": "Content-Type",
						"name": "Content-Type",
						"value": "application/json",
						"type": "text"
					}
				],
				"body": {
					"mode": "raw",
					"raw": "{\r\n    \"name\": \"{{skillset-name}}\",\r\n    \"description\": \"Skillset\",\r\n    \"skills\": [\r\n        {\r\n            \"@odata.type\": \"#Microsoft.Skills.Custom.WebApiSkill\",\r\n            \"name\": \"UploadToQnA\",\r\n            \"description\": \"Upload the documents to the QnA knowledge base\",\r\n            \"uri\": \"https://{{function-name}}.azurewebsites.net/api/upload-to-qna?code={{function-code}}\",\r\n            \"httpMethod\": \"POST\",\r\n            \"context\": \"/document\",\r\n            \"batchSize\": 10,\r\n            \"timeout\": \"PT230S\",\r\n            \"degreeOfParallelism\": 1,\r\n            \"inputs\": [\r\n                {\r\n                    \"name\": \"id\",\r\n                    \"source\": \"/document/id\"\r\n                },\r\n                {\r\n                    \"name\": \"blobName\",\r\n                    \"source\": \"/document/metadata_storage_name\"\r\n                },\r\n                {\r\n                    \"name\": \"blobUrl\",\r\n                    \"source\": \"/document/metadata_storage_path\"\r\n                },\r\n                {\r\n                    \"name\": \"sasToken\",\r\n                    \"source\": \"/document/metadata_storage_sas_token\"\r\n                }\r\n            ],\r\n            \"outputs\": [\r\n                {\r\n                    \"name\": \"status\"\r\n                }\r\n            ]\r\n        }\r\n    ],\r\n    \"cognitiveServices\": {\r\n        \"@odata.type\": \"#Microsoft.Azure.Search.CognitiveServicesByKey\",\r\n        \"description\": \"CognitiveServices AllInOne account\",\r\n        \"key\": \"{{cog-svc-allinone-key}}\"\r\n    }\r\n}",
					"options": {
						"raw": {
							"language": "json"
						}
					}
				},
				"url": {
					"raw": "https://{{search-service-name}}.search.windows.net/skillsets/{{skillset-name}}?api-version={{api-version}}",
					"protocol": "https",
					"host": [
						"{{search-service-name}}",
						"search",
						"windows",
						"net"
					],
					"path": [
						"skillsets",
						"{{skillset-name}}"
					],
					"query": [
						{
							"key": "api-version",
							"value": "{{api-version}}"
						}
					]
				}
			},
			"response": []
		},
		{
			"name": "Create Indexer",
			"request": {
				"method": "PUT",
				"header": [
					{
						"key": "api-key",
						"value": "{{search-service-admin-key}}",
