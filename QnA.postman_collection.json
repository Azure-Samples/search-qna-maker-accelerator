					"raw": "{\r\n    \"name\": \"{{datasource-name}}\",\r\n    \"description\": \"QnA docs\",\r\n    \"type\": \"azureblob\",\r\n    \"credentials\": {\r\n        \"connectionString\": \"{{storage-connection-string}}\"\r\n    },\r\n    \"container\": {\r\n        \"name\": \"{{storage-container-name}}\"\r\n    }\r\n}",
